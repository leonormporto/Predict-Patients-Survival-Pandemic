  },
  {
   "cell_type": "code",
   "execution_count": 28,
   "metadata": {},
   "outputs": [],
   "source": [
    "labelencoder = LabelEncoder()\n",
    "\n",
    "# TRAIN\n",
    "train['Medical_Tent'] = labelencoder.fit_transform(train['Medical_Tent'])\n",
    "# TEST\n",
    "test['Medical_Tent'] = labelencoder.fit_transform(test['Medical_Tent'])"
   ]
